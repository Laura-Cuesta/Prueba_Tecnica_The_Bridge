    "## CREACION DB\n",
    "#############################################\n",
    "conn = sqlite3.connect('database_twitter')\n",
    "c = conn.cursor()\n",
    "## CREACION 2 TABLAS\n",
    "#############################################\n",
    "c.execute('''CREATE TABLE IF NOT EXISTS twitter1 \n",
    "  (\n",
    "  nombre_autor TEXT(40),\n",
    "  username_autor TEXT(20),\n",
    "  texto_tw TEXT(140),\n",
    "  id_tw INTEGER(20) NOT NULL,   \n",
    "  id_autor INTEGER(20),\n",
    "  fecha_creacion TEXT(20),\n",
    "  PRIMARY KEY(username_autor),\n",
    "  FOREIGN KEY(id_tw) REFERENCES twitter2(id_tw))\n",
    "  ''');\n",
    "\n",
    "  #id_tw in twitter1 as NOT NULL because it is primary key in twitter2\n",
    "\n",
    "c.execute('''CREATE TABLE IF NOT EXISTS twitter2 \n",
    "  (id_tw INTEGER(20),\n",
    "  num_retw INTEGER(10),\n",
    "  num_contestacion INTEGER(10),\n",
    "  num_likes INTEGER(10),\n",
    "  num_menciones INTEGER(10),\n",
    "  id_autor INTEGER(20),\n",
    "  PRIMARY KEY(id_tw))\n",
    "  ''');\n",
    "\n",
    "#INSERTAR DATOS DATAFRAME > TABLAS\n",
    "\n",
    "info_twitter1.to_sql('twitter1', conn, if_exists='replace', index = False)\n",
    "info_twitter2.to_sql('twitter2', conn, if_exists='replace', index = False)\n",
    "\n",
    "conn.commit()\n",
    "\n",
    "## COMPROBACION DE LAS DOS TABALS\n",
    "c.execute(\"SELECT * FROM twitter1\")\n",
    "for row in c.fetchall():\n",
    "    print (row)\n",
    "\n",
    "c.execute(\"SELECT * FROM twitter2\")\n",
    "for row in c.fetchall():\n",
    "    print (row)\n",
    "\n",
    "conn.close()"
   ]
